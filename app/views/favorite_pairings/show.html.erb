<div class="page-header">
  <h3>Favorite Pairing #<%= @favorite_pairing.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @favorite_pairing.user_id %></dd>

      <dt>Pairing</dt>
      <dd><%= @favorite_pairing.pairing_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/favorite_pairings" class="btn btn-primary">
        Back
      </a>
      <a href="/favorite_pairings/<%= @favorite_pairing.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_favorite_pairing/<%= @favorite_pairing.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
